//задание1
#include <stdio.h>

int main()
{
    for(int a=0; a<=100; a++){
        if (a%3==0)
        printf("%d\n", a);
        }
    return 0;
}

//задание2
#include <stdio.h>

int main()
{
    int s=0, a=1;
  
    while (a<=1000){
        if (a%7==0)
        s=s+a;
        a=a+4;;
        }
    printf("%d\n", s);
    return 0;
}
