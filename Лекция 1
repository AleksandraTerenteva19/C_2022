#include <zadacha1.h>

int main()
{
    %целое число
    int a;
    a = 15;
    printf("Значение а: %d", a);
    
    %число с плавающей точкой
    float a;
    a = 20;
    printf("Значение а: %d", a);
    
    %символ
    char a;
    a = '9';
    printf("Значение а: %d", a);
    
    %число с плавающей точкой двойной точности
    double a;
    a = 20;
    printf("Значение а: %d", a);
    
    return 0;
    }
    
